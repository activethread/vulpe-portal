<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 3.2//EN"
        "http://tuckey.org/res/dtds/urlrewrite3.2.dtd">
<urlrewrite>
	<rule>
		<from>^/([a-z]+)/([a-z]+)/([a-z]+)/([0-9]+)</from>
		<to>/$1/$2/$3?id=$4</to>
	</rule>
	<rule>
		<from>^/([a-z]+)/([a-z]+)/([a-z]+)/ajax/([0-9]+)</from>
		<to>/$1/$2/$3/ajax?id=$4</to>
	</rule>
	<rule>
		<from>^/([a-z]+)/([a-z]+)/update/([0-9]+)</from>
		<to>/$1/$2/update?id=$3</to>
	</rule>
	<rule>
		<from>^/([a-z]+)/([a-z]+)/update/ajax/([0-9]+)</from>
		<to>/$1/$2/update/ajax?id=$3</to>
	</rule>
	<rule>
		<from>^/([a-z]+)/([a-z]+)/view/([0-9]+)</from>
		<to>/$1/$2/view?id=$3</to>
	</rule>
	<rule>
		<from>^/([a-z]+)/([a-z]+)/view/ajax/([0-9]+)</from>
		<to>/$1/$2/view/ajax?id=$3</to>
	</rule>
	<rule>
		<from>^/([a-z]+)/([a-z]+)/([0-9]+)</from>
		<to>/$1/$2/update?id=$3</to>
	</rule>
	<rule>
		<from>^/([a-z]+)/([a-z]+)/ajax/([0-9]+)</from>
		<to>/$1/$2/update/ajax?id=$3</to>
	</rule>
</urlrewrite>